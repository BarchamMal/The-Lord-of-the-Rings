{
  "multipart": [
    {
      "apply": {
        "model": "the-lord-of-the-rings-mod-biomes:block/{wood_type}_fence_post"
      }
    },
    {
      "apply": {
        "model": "the-lord-of-the-rings-mod-biomes:block/{wood_type}_fence_side",
        "uvlock": true
      },
      "when": {
        "north": "true"
      }
    },
    {
      "apply": {
        "model": "the-lord-of-the-rings-mod-biomes:block/{wood_type}_fence_side",
        "uvlock": true,
        "y": 90
      },
      "when": {
        "east": "true"
      }
    },
    {
      "apply": {
        "model": "the-lord-of-the-rings-mod-biomes:block/{wood_type}_fence_side",
        "uvlock": true,
        "y": 180
      },
      "when": {
        "south": "true"
      }
    },
    {
      "apply": {
        "model": "the-lord-of-the-rings-mod-biomes:block/{wood_type}_fence_side",
        "uvlock": true,
        "y": 270
      },
      "when": {
        "west": "true"
      }
    }
  ]
}